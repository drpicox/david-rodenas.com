<angular-element name="tag-list" bindings="tagnames">
	<style>
		tag-list .md-button {
			display: block;
			margin: 0 .5em -0.4em 0;
		}
	</style>
	<template layout="row" layout-wrap>
		<md-button
		ng-repeat="tagname in vm.tagnames"
		ng-href="#/tags/{{tagname}}" 
		ng-bind="tagname" 
		aria-label="tag {{tagname}}"
		></md-button>
	</template>
</angular-element>