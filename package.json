{
  "name": "gatsby-drpicox-home",
  "private": true,
  "description": "A simplified home page",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "gatsby clean; yarn develop",
    "serve": "gatsby serve",
    "ldeploy": "gatsby clean && gatsby build && gh-pages -d public -b master",
    "pubx": "gh-pages -d public -b master",
    "deploy": "gatsby clean && gatsby build && gh-pages -d public -b master -r https://$GH_TOKEN@github.com/drpicox/drpicox.github.io.git",
    "pub": "gh-pages -d public -b master -r https://$GH_TOKEN@github.com/drpicox/drpicox.github.io.git",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "gh-pages-clean": "gh-pages-clean"
  },
  "dependencies": {
    "@emotion/core": "^10.1.1",
    "@mdx-js/mdx": "^1.6.19",
    "@mdx-js/react": "^1.6.19",
    "gatsby": "^2.25.3",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-client-side-redirect": "^1.1.0",
    "gatsby-plugin-emotion": "^4.3.15",
    "gatsby-plugin-google-analytics": "^2.4.0",
    "gatsby-plugin-manifest": "^2.5.2",
    "gatsby-plugin-mdx": "^1.3.1",
    "gatsby-plugin-page-creator": "^2.3.35",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.7.1",
    "gatsby-plugin-sitemap": "^2.5.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "^3.6.0",
    "gatsby-source-filesystem": "^2.4.2",
    "gatsby-transformer-sharp": "^2.5.21",
    "gh-pages": "^3.1.0",
    "mermaid": "^9.1.2",
    "object-path-immutable": "^4.1.1",
    "prism-themes": "^1.5.0",
    "prismjs": "^1.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-use": "^15.3.4",
    "spark-md5": "^3.0.2",
    "unist-util-visit": "^2.0.1"
  },
  "devDependencies": {
    "prettier": "^2.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/drpicox/david-rodenas.com.git"
  },
  "bugs": {
    "url": "https://github.com/drpicox/david-rodenas.com/issues"
