{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "gatsby clean; yarn develop",
    "serve": "gatsby serve",
    "ldeploy": "gatsby build && gh-pages -d public -b master",
    "deploy": "gatsby build && gh-pages -d public -b master -r https://$GH_TOKEN@github.com/drpicox/drpicox.github.io.git",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@mdx-js/mdx": "^1.5.1",
    "@mdx-js/react": "^1.5.1",
    "gatsby": "^2.15.34",
    "gatsby-image": "^2.2.27",
    "gatsby-plugin-emotion": "^4.1.10",
    "gatsby-plugin-google-analytics": "^2.1.21",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-mdx": "^1.0.50",
    "gatsby-plugin-page-creator": "^2.1.25",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-sitemap": "^2.2.17",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-prismjs": "^3.3.18",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-sharp": "^2.2.21",
    "gh-pages": "^2.1.1",
    "prismjs": "^1.17.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-use": "^12.4.0",
    "unist-util-visit": "^2.0.0"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "learnMore": {
    "mdx": "https://alligator.io/gatsbyjs/mdx-in-gatsby/"
  }
}
